{
  "name": "insight-backoffice",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:local": "ng serve --configuration local",
    "start:dev": "ng serve --configuration development --port 54388",
    "build": "ng build --configuration production --output-path=dist",
    "build:dev": "ng build --configuration development --output-path=dist --base-href /dev/backoffice/",
    "build:prod": "ng build --configuration production --output-path=dist --base-href /backoffice/",
    "watch": "ng build --watch --configuration development",
    "serve": "http-server dist/ -p 4200",
    "test": "ng test --browsers=\"ChromeHeadless\" --watch=false",
    "get-types": "graphql-codegen",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,js,graphql}\"",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@apollo/client": "^3.10.4",
    "@ngrx/store": "^18.0.0-beta.1",
    "@ngx-env/builder": "^18.0.1",
    "apollo-angular": "^7.0.2",
    "graphql": "^16.8.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.4.5"
  },
  "lint-staged": {
    "*.{js,css,ts}": "prettier --write"
  }
}
